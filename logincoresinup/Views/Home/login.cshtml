

@model logincoresinup.Models.Emplogin
@{
    ViewData["Title"] = "login";
    Layout = "~/Views/Shared/_Layout_login.cshtml";
}
<style>
    .st {
        color: red;
    }
</style>




<h1>login</h1>

<form asp-action="login" asp-controller="Home" method="post">
    <div class="form-group">
        <label>UserName</label>
        <input asp-for="UserName" type="text" class="form-control" autocomplete="off" />
        <span class="st">@TempData["email"]</span>
    </div>
    <div class="form-group">
        <label>UserPassword</label>
        <input asp-for="UserPassword" type="password" class="form-control" autocomplete="off" />
        <span class="st">@TempData["pass"]</span>
    </div>



    <button type="submit" class="btn btn-primary">login </button>
    <button type="reset" class="btn btn-success">Reset</button>
    <a asp-action="sinup" asp-controller="Home" class="btn btn-danger">SinUp</a>
</form>

